<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
  </configSections>
  <startup>
    
  <supportedRuntime version="v2.0.50727"/></startup>
  <superSocket xmlns="http://schema.supersocket.net/supersocket" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schema.supersocket.net/supersocket
               http://schema.supersocket.net/v1-6/supersocket.xsd" minCompletionPortThreads="20">
    <servers>
      <server name="RainfallServer" serverTypeName="Protocol" ip="Any" port="12000" maxRequestLength="2048" maxConnectionNumber="10000" logCommand="true" logBasicSessionActivity="true">
      </server>
    </servers>
    <serverTypes>
      <add name="Protocol" type="Protocol.RainfallServer, Protocol"/>
    </serverTypes>
  </superSocket>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>
  <appSettings>
    <add key="ServiceName" value="Protocol"/>
  </appSettings>
</configuration>
